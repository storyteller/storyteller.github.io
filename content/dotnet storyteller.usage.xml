<?xml version="1.0" encoding="utf-8"?>
<CommandLineApplicationReport xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ApplicationName>dotnet storyteller</ApplicationName>
  <Commands>
    <CommandReport>
      <Name>open</Name>
      <Description>Opens the specification editor web tool</Description>
      <Arguments />
      <Flags>
        <FlagReport>
          <UsageDescription>[-f, --framework &lt;framework&gt;]</UsageDescription>
          <Description>Override the .Net target framework if you are multi-targeting</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-h, --hotreload]</UsageDescription>
          <Description>Only for Storyteller development itself</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-s, --specs &lt;specs&gt;]</UsageDescription>
          <Description>Optional. Override the spec directory</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-y, --path &lt;path&gt;]</UsageDescription>
          <Description>Optional, override the root directory of the project. Should be very rarely used</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[----prop:&lt;prop&gt; &lt;value&gt;]</UsageDescription>
          <Description>Key/Value data for customized system construction</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --profile &lt;profile&gt;]</UsageDescription>
          <Description>Storyteller Profile for conditional system construction</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-c, --culture &lt;culture&gt;]</UsageDescription>
          <Description>Force Storyteller to use this culture in all value conversions</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-f, --fixtures &lt;fixtures&gt;]</UsageDescription>
          <Description>Optional. Override the fixtures directory</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Open the Storyteller client to the current project</Description>
          <Usage>dotnet storyteller open [-f, --framework &lt;framework&gt;] [-h, --hotreload] [-s, --specs &lt;specs&gt;] [-y, --path &lt;path&gt;] [----prop:&lt;prop&gt; &lt;value&gt;] [-p, --profile &lt;profile&gt;] [-c, --culture &lt;culture&gt;] [-f, --fixtures &lt;fixtures&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
  </Commands>
</CommandLineApplicationReport>